---
const { activeTab, profile } = Astro.props;

const activeTabClasses =
  "inline-flex p-4 text-gray-600 border-b-2 border-gray-600 rounded-t-lg active dark:text-gray-500 dark:border-gray-500 group";
const tabClasses =
  "inline-flex p-4 border-b-2 border-transparent rounded-t-lg hover:text-gray-600 hover:border-gray-300 dark:hover:text-gray-300 group";

const tabsNames = ["profile", "cv", "blog", "projects", "apps", "services"];
let tabsClasses = {};

for (const tab of tabsNames) {
  tabsClasses[tab] = tab == activeTab ? activeTabClasses : tabClasses;
}
---

<div>
  <div class="border-b border-gray-200 dark:border-gray-700">
    <ul class="flex flex-wrap -mb-px text-sm justify-center font-medium text-center text-gray-500 dark:text-gray-100">
      <li class="mr-2">
        <a href={"/dev/"+profile.username} class={tabsClasses["profile"]} aria-current="page">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="currentColor"
            class="w-5 h-5 mr-2 text-gray-600 dark:text-gray-100"
            viewBox="0 0 24 24"
            ><g fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"
              ><path d="M16 9a4 4 0 1 1-8 0a4 4 0 0 1 8 0Zm-2 0a2 2 0 1 1-4 0a2 2 0 0 1 4 0Z"></path><path
